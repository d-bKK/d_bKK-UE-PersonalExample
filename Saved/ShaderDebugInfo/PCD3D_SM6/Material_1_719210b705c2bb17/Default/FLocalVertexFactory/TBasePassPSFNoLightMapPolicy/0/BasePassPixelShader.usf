{
	float SinAlpha = sqrt( SinAlphaSqr );
	if( NoL < SinAlpha )
	{
		NoL = max( NoL, -SinAlpha );
		NoL = Pow2( SinAlpha + NoL ) / ( 4 * SinAlpha );
	}
	return NoL;
