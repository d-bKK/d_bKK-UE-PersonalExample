uint PackHairCurve(FHairCurve In)
{
	return (In.PointIndex & ((1u<<24)-1u)) | (In.PointCount << 24);
}
FHairCurve UnpackHairCurve(uint In)
{
	FHairCurve Out = (FHairCurve)0;
	Out.PointIndex = In & ((1u<<24)-1u);
	Out.PointCount = In >> 24;
