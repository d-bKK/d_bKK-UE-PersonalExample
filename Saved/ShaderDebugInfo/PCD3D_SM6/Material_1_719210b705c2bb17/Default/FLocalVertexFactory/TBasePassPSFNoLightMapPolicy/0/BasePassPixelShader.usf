uint MaskedBitCount( uint2 Bits, uint Index )
{
	bool bLow = Index < 32;
	uint Mask = 1u << ( Index - ( bLow ? 0 : 32 ) );
	Mask -= 1;
	uint Offset;
	Offset  = countbits( Bits.x & ( bLow ? Mask : ~0u ) );
	Offset += countbits( Bits.y & ( bLow ? 0 : Mask ) );
	return Offset;
}
