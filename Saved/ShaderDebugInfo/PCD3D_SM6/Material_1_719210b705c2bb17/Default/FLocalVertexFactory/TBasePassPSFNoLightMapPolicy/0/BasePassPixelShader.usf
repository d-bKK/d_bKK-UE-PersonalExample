	const float3 P1 =   MakePrecise(  mad( ( -Sh ) , ( Sh ) , ( V.High ) ) ); 
	const float3 P2 =   MakePrecise( ( V.Low ) + ( P1 ) );
	const float3 Sl =   MakePrecise( ( P2 ) / ( (2.0 * Sh) ) );
	return DFFastTwoSum(Sh, Sl);
}
float3 DFSqrtDemote(FDFVector3 V)
{
